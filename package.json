{
    "name": "skola24-tailor-made-calendar",
    "version": "1.0.0",
    "description": "Webscraper for skola24 schemavisare to export ics files, with your custom-made structure",
    "keywords": [
        "webscraper",
        "schedule",
        "calendar",
        "ics",
        "skola24"
    ],
    "main": "src/server.js",
    "scripts": {
        "start": "npx concurrently -n browser,scss,node npm:start-browser npm:watch-scss npm:watch-node",
        "start-browser": "bash ./start-browser.sh",
        "watch-scss": "npx sass --watch public/scss:public/css",
        "watch-node": "npx nodemon . | npx pino-pretty"
    },
    "author": "David Lindstr√∂m",
    "license": "MIT License",
    "dependencies": {
        "date-fns": "^2.21.3",
        "express": "^4.17.1",
        "ics": "^2.31.0",
        "morgan": "^1.10.0",
        "pino-http": "^5.5.0",
        "pino-pretty": "^4.8.0",
        "puppeteer": "^9.1.1",
        "puppeteer-extra": "^3.1.18",
        "puppeteer-extra-plugin-stealth": "^2.7.8",
        "sass": "^1.38.0"
    },
    "devDependencies": {
        "concurrently": "^6.2.1",
        "nodemon": "^2.0.7"
    }
}
